<div class="review-rating" data-score="<%= review.rating %>">

</div>
<!-- <p><%= review.rating %></p> -->
<p><%= review.comment %></p>

<% if user_signed_in? %>
  <% if review.user_id == current_user.id%>
    <%= link_to "编辑", edit_book_review_path(review.book,review) %>
    <%= link_to "删除", book_review_path(review.book,review),method: :delete,
        data:{confirm:"你确定吗？"} %>
  <%end%>
<%end%>
